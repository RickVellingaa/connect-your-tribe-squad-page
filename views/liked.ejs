<%- include('./partials/head') %>
<nav>
    <%- include('./partials/navigation') %>
</nav>
<main>
    <div class="detail">
        <ion-icon class="playlists__menu" name="menu-outline"></ion-icon>
        <section class="detail__header">
            <img class="detail__logo" src="/assets/liked-student.png" alt="Logo - Squad A">
            <div class="detail__squad">
                <span class="squad__usp">Playlist</span>
                <h1 class="squad__title">Gelikte studenten</h1>
                <p class="squad__description">Alle studenten die jij hebt geliked!</p>
                <p class="squad__amount"><img src="/assets/liked-student.png" alt="Logo van squad A">Dit zijn alle
                    studenten die jij een hartje hebt gegeven!</p>
            </div>
        </section>
        <div class="students">
            <table>
                <tbody>
                    <tr>
                        <th>#</th>
                        <th>Student</th>
                        <th>Squad</th>
                        <th>Github</th>
                        <th>
                            <ion-icon name="time-outline"></ion-icon>
                        </th>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</main>

<!-- Link naar JS -->
<script src="./scripts/script.js"></script>

<%- include('./partials/foot') %>